{
  "name": "raw-decoder",
  "version": "1.1.0",
  "description": "Decodes data from RAW camera files.",
  "main": "./dist/decoder.js",
  "types": "./dist/decoder.d.ts",
  "module": "./dist-es6/decoder.js",
  "scripts": {
    "build": "tsc && tsc -p tsconfig-es6.json",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:unit": "mocha --reporter spec 'test/**/*.test.js'",
    "test:lint": "lint -t typescript --ignore 'lib/**/*.d.ts' 'lib/**/*.ts'",
    "test:update-expectations": "UPDATE_EXPECTATIONS=true npm run test:unit",
    "test:coverage": "nyc npm run test:unit",
    "test:watch": "mocha --watch --reporter dot 'test/**/*.test.js'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eris-ai/js-raw-decoder.git"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eris-ai/js-raw-decoder/issues"
  },
  "homepage": "https://github.com/eris-ai/js-raw-decoder#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@patrickhulce/lint": "^2.1.2",
    "@types/lodash": "^4.14.63",
    "@types/node": "^7.0.16",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "mocha": "^3.3.0",
    "nyc": "^10.3.0",
    "semantic-release": "^6.3.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.10.0",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "debug": "^2.6.6",
    "exif-parser": "0.1.9"
  }
}
