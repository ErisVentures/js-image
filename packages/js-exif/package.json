{
  "name": "@ouranos/exif",
  "version": "0.0.0",
  "description": "Parses EXIF data.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rm -fR dist/",
    "rebuild": "npm run clean && npm run build",
    "prepublish": "npm run rebuild",
    "build": "babel lib --out-dir dist/ && BABEL_ENV=rollup rollup -c rollup.config.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "mocha --reporter spec 'test/**/*.test.js'",
    "test:lint": "lint",
    "test:coverage": "nyc npm run test:unit",
    "test:watch": "mocha --watch --reporter dot 'test/**/*.test.js'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ouranos-oss/js-exif.git"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ouranos-oss/js-exif/issues"
  },
  "homepage": "https://github.com/ouranos-oss/js-exif#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@patrickhulce/lint": "^1.1.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "cz-conventional-changelog": "^2.0.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "semantic-release": "^6.3.6",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "dependencies": {
    "exif-parser": "^0.1.9",
    "raw-decoder": "^1.0.0"
  }
}
